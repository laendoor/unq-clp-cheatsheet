{
  "ax_v":    { "prefix": "axv",       "body": "\\infer[ax_v]{$1}{}"  },
  "ax_c":    { "prefix": "axc",       "body": "\\infer[ax_c]{$1}{}"  },
  "\\vdash": { "prefix": ["context"], "body": "\\vdash "  },
  "\\infer": { "prefix": "infer",     "body": "\\infer[]\n\t{$1}\n\t{$2}" },

  "\\Gamma \\vdash":    { "prefix": ["gamma-context"],  "body": "\\Gamma \\vdash "  },
  "\\begin{align*}":    { "prefix": "align",            "body": "\\begin{align*}\n\t& $1\n\\end{align*}"  },
  "\\begin{equation*}": { "prefix": "equation",         "body": "\\begin{equation*}\n\t$1\n\\end{equation*}"  },
  "\\begin{enumerate}": { "prefix": "enumerate",        "body": "\\begin{enumerate}\n\t\\item $1\n\\end{enumerate}"  },
  "\\begin{itemize}":   { "prefix": "itemize",          "body": "\\begin{itemize}\n\t\\item $1\n\\end{itemize}"  },
  "\\rightarrow":       { "prefix": "arrow",            "body": "\\rightarrow"  },
  "\\rightsquigarrow":  { "prefix": "squidarrow",       "body": "\\rightsquigarrow"  },
  "\\Rightarrow":       { "prefix": ["then", "flecha"], "body": "\\then "  },

  "if zero": { "prefix": "ifz", "body": "\\ifz z t e"  },
  "let":     { "prefix": "let", "body": "\\letl x u t"  },
  "mathsf":  { "prefix": "sf",  "body": "\\s{$1}"      },

  "\\nat":     { "prefix": "nat",     "body": "\\nat "           },
  "\\Gamma":   { "prefix": "gamma",   "body": "\\Gamma "         },
  "\\Add":     { "prefix": "add",     "body": "\\Add "           },
  "\\Apply":   { "prefix": "apply",   "body": "\\Apply "         },
  "\\Div":     { "prefix": "div",     "body": "\\Div "           },
  "\\Extend":  { "prefix": "extend",  "body": "\\Extend "        },
  "\\Ldi":     { "prefix": "ldi",     "body": "\\Ldi {$1} "      },
  "\\Mkclos":  { "prefix": "mkclos",  "body": "\\Mkclos {$1}"    },
  "\\Mult":    { "prefix": "mult",    "body": "\\Mult "          },
  "\\Popenv":  { "prefix": "popenv",  "body": "\\Popenv "        },
  "\\Push":    { "prefix": "push",    "body": "\\Push "          },
  "\\Pushenv": { "prefix": "pushenv", "body": "\\Pushenv "       },
  "\\Search":  { "prefix": "search",  "body": "\\Search {$1}"    },
  "\\Sub":     { "prefix": "sub",     "body": "\\Sub "           },
  "\\Test":    { "prefix": "test",    "body": "\\Test {$1} {$2}" },

  "compile expression":  { "prefix": "comp:expr",   "body": "\\Search n" },
  "compile application": { "prefix": "comp:app",    "body": "\\Pushenv;\n|u|_{e};\n\\Push;\n|t|_{e};\n\\Apply;\n\\Popenv" },
  "compile function":    { "prefix": "comp:fun",    "body": "\\Mkclos {|t|_{e\\concats[\\_,x]}}" },
  "compile fixfun":      { "prefix": "comp:fixfun", "body": "\\Mkclos {|t|_{e\\concats[f,x]}}" },
  "compile number":      { "prefix": "comp:number", "body": "\\Ldi n" },
  "compile sum":         { "prefix": "comp:sum",    "body": "|u|_{e};\n\\Push;\n|t|_{e};\n\\Add" },
  "compile sub":         { "prefix": "comp:sub",    "body": "|u|_{e};\n\\Push;\n|t|_{e};\n\\Sub" },
  "compile mult":        { "prefix": "comp:mult",   "body": "|u|_{e};\n\\Push;\n|t|_{e};\n\\Mult" },
  "compile div":         { "prefix": "comp:div",    "body": "|u|_{e};\n\\Push;\n|t|_{e};\n\\Div" },
  "compile ifz":         { "prefix": "comp:ifz",    "body": "|t|_{e};\n\\Test {|u|_{e}} {|v|_{e}})" },
  "compile let":         { "prefix": "comp:let",    "body": "\\Pushenv;\n|t|_{e};\n\\Extend;\n|u|_{e\\concats [x]};\n\\Popenv" },

  "semantic _init":       { "prefix": "sem:init",     "body": "\\semT[\\emptyset]{$1}" },
  "semantic number":      { "prefix": "sem:num",      "body": "n" },
  "semantic expression":  { "prefix": "sem:expr",     "body": "$1x" },
  "semantic function":    { "prefix": "sem:fun",      "body": "\\funSem{a}{$1A}{\\semT[$2x=a]{$3t}}" },
  "semantic fun-math":    { "prefix": "sem:fun:math", "body": "\\funSemR{a}{$1\\N}{\\semT[$2x=a]{$3t}}" },
  "semantic application": { "prefix": "sem:app",      "body": "\\semT[$1]{$3t} \\semT[$2]{$4r}" },
  "semantic operation":   { "prefix": "sem:oper",     "body": "\\semT[$1]{t} \\otimes \\semT[$2]{r}" },
  "semantic ifz":         { "prefix": "sem:ifz",      "body": "\\left\\{\\begin{array}{ll}\n\t\\semT[$1]{r} & \\textrm{si }\\semT[$2]{t} = 0 \\\\\\\\\n\t\\semT[$3]{s} & \\textrm{si }\\semT[$4]{t} \\in \\N^* \\\\\\\\\n\t\\bot_{A} & \\textrm{si }\\semT[$5]{t} = \\bot_{\\N}\n\\end{array}\\right." },
  "semantic let":         { "prefix": "sem:let",      "body": "\\semT[x=\\semT[$1]{r}]{t}" },
  "semantic fixfun":      { "prefix": "sem:fix",      "body": "\\FIX(\\funSem{a}{$1A}{\\semT[$2x=a]{$3t}})" },
  "semantic fixfun-math": { "prefix": "sem:fix:math", "body": "\\FIX(\\funSemR{a}{$1\\N}{\\semT[$2x=a]{$3t}})" },
  "bottom": { "prefix": "bottom", "body": "\\bot_{$1\\N}" }
}
